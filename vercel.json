{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "https://www.aryanaligeti.dev/(.*)",
      "destination": "https://aryanaligeti.dev/$1",
      "permanent": true
    }
  ]
}
