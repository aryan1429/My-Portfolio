{
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "OPENAI_API_KEY": "@openai_api_key",
    "CONTACT_EMAIL": "aryanaligetibusiness@gmail.com"
  }
}